{
  "name": "mero/delphi-compat",
  "type": "library",
  "description": "Interpret data generated by Delphi resources.",
  "keywords": [
    "Delphi",
    "Compat",
    "Mero",
    "TColor"
  ],
  "homepage": "https://github.com/merorafael/DelphiCompat",
  "license": "MIT",
  "authors": [
    {
      "name": "Rafael Mello",
      "email": "merorafael@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.6"
  },
  "autoload": {
    "psr-4": {
      "Mero\\DelphiCompat\\": "src/Mero/DelphiCompat"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mero\\DelphiCompat\\": "tests/Mero/DelphiCompat"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  },
  "scripts": {
    "test": [
      "phpunit"
    ]
  }
}
